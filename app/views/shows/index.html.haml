-content_for :body_content_left do
  #shows
    -@shows.each do |year, shows|
      %h2.year=year
      -shows.each do |show|
        - title = "#{show.name} <span class='date'>#{show.date.strftime('%A, %b %e, %Y')}</span>"
        .show= link_to image_tag(show.flyer.url, title: title), show
      .clear
= render "/shared/content_page"
